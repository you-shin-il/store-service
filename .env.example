# ============================================================
# 환경변수 템플릿 — .env로 복사 후 실제 값 입력
# cp .env.example .env
# ============================================================

# 네이버 SMTP
NAVER_EMAIL=your_id@naver.com
NAVER_PASSWORD=your_password

# JWT
JWT_SECRET=your-jwt-secret-key-must-be-at-least-256-bits-long-for-hs256

# PostgreSQL (Docker Compose 기본값과 동일)
DB_URL=jdbc:postgresql://localhost:5432/store_service
DB_USERNAME=store
DB_PASSWORD=store1234

# MinIO
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_ENDPOINT=http://localhost:9000
